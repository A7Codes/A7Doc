Public Class Apt

    Public name As String
    Public phone As String
    Public dat As String
    Public tim As String
    Public id As String

    Public Sub New(ByVal id As String, ByVal name As String, ByVal phone As String, ByVal dat As String, ByVal tim As String)

        Me.id = id
        Me.name = name
        Me.phone = phone
        Me.dat = dat
        Me.tim = tim

    End Sub


End Class
